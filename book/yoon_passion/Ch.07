#include <stdio.h>

int main(void)
{
    int num;
    int NUM=0, total=0;

    while(NUM<5)
    {
        printf("정수를 입력하시오:");
        scanf("%d", &num);

        while(num<1)
        {
            continue;
        }

        total+=num;
        NUM++;

    }
    printf("%d", total);
    return 0;
}
